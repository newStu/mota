{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "types": [
      "element-plus/global",
      "@vue-macros/reactivity-transform/macros-global"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
